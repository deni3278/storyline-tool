<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<ScrollPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="storyline.controller.TimelineListViewController">
   <content>
      <VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0">
         <children>
            <ListView id="timelineListView" fx:id="timelineListView" prefHeight="415.0" prefWidth="600.0" />
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button fx:id="loadButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="47.0" text="Load" />
                  <Button fx:id="closeButton" mnemonicParsing="false" text="Close" />
               </children>
            </HBox>
         </children>
      </VBox>
   </content>
</ScrollPane>
